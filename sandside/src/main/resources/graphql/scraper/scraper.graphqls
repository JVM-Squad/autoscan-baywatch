type AtomFeed {
    id: ID,
    title: String,
    description: String,
    author: String,
    link: URI!
}

type AtomEntry {
    id: ID,
    title: String,
    description: String,
    author: String,
    link: URI!
    feeds: [String]
}

type ScrapingError {
    code: Int,
    since: String,
    lastTime: String,
    message: String,
}

extend type Query {
    scrapFeedHeader(link: URI): AtomFeed
}

extend type Mutation {
    scrapSimpleNews(uri: URI): Void
}